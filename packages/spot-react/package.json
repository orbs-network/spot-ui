{
  "name": "@orbs-network/spot-react",
  "version": "0.0.16",
  "description": "Spot React Components - React components for Spot SDK",
  "type": "module",
  "main": "./dist/spot-react.umd.cjs",
  "module": "./dist/spot-react.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/spot-react.js",
      "require": "./dist/spot-react.umd.cjs"
    },
    "./styles.css": "./dist/style.css"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "vite build --watch",
    "build": "tsc && vite build",
    "prepublishOnly": "pnpm run build"
  },
  "keywords": [
    "spot",
    "ui",
    "react",
    "components"
  ],
  "license": "MIT",
  "peerDependencies": {
    "react": "^18.0.0 || ^19.0.0",
    "react-dom": "^18.0.0 || ^19.0.0"
  },
  "dependencies": {
    "@orbs-network/spot-ui": "workspace:*",
    "@orbs-network/swap-ui": "^0.0.61",
    "@react-icons/all-files": "^4.1.0",
    "@tanstack/react-query": "^5.90.12",
    "bignumber.js": "^9.3.1",
    "moment": "^2.30.1",
    "react-error-boundary": "^6.0.0",
    "react-number-format": "^5.4.4",
    "react-virtuoso": "^4.17.0",
    "viem": "^2.43.3",
    "zustand": "^5.0.9"
  },
  "devDependencies": {
    "@types/react": "^19.0.2",
    "@types/react-dom": "^19.0.2",
    "@vitejs/plugin-react": "^4.3.4",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "typescript": "^5.7.2",
    "vite": "^6.0.5",
    "vite-plugin-dts": "^4.4.0"
  }
